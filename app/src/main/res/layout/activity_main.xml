<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <EditText android:layout_width="209dp" android:layout_height="70dp"
              android:id="@+id/editId" android:hint="Digite o CEP" tools:layout_editor_absoluteY="34dp"
              tools:layout_editor_absoluteX="101dp"/>

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/btPesquisar" android:text="Pesquisar"
            android:onClick="pesquisar"
            app:layout_constraintTop_toBottomOf="@+id/editId" android:layout_marginTop="12dp"
            tools:layout_editor_absoluteX="143dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>